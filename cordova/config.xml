<?xml version='1.0' encoding='utf-8'?>
<widget id="com.ixiaer.hicordova" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
  <name>HiCordova</name>
  <description>
    A sample Apache Cordova application that responds to the deviceready event.
  </description>
  <author email="dev@cordova.apache.org" href="http://cordova.io">
    Apache Cordova Team
  </author>
  <content src="index.html"/>
  <plugin name="cordova-plugin-whitelist" spec="1"/>
  <access origin="*"/>
  <allow-intent href="http://*/*"/>
  <allow-intent href="https://*/*"/>
  <allow-intent href="tel:*"/>
  <allow-intent href="sms:*"/>
  <allow-intent href="mailto:*"/>
  <allow-intent href="geo:*"/>
  <platform name="android">
    <icon density="mdpi" src="res/mdpi/ic_launcher_APP.png"/>
    <icon density="hdpi" src="res/hdpi/ic_launcher_APP.png"/>
    <icon density="xhdpi" src="res/xhdpi/ic_launcher_APP.png"/>
    <icon density="xxhdpi" src="res/xxhdpi/ic_launcher_APP.png"/>
    <icon density="xxxhdpi" src="res/xxxhdpi/ic_launcher_APP.png"/>
    <allow-intent href="market:*"/>
  </platform>
  <platform name="ios">
    <allow-intent href="itms:*"/>
    <allow-intent href="itms-apps:*"/>
  </platform>
  <preference name="Orientation" value="landscape" />
</widget>
