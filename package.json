{
  "name": "hicordova",
  "version": "0.0.1",
  "description": "hicordova",
  "author": "Chenglin Jin",
  "license": "MIT",
  "scripts": {
    "serve": "cross-env NODE_ENV=development webpack-dev-server --progress",
    "android": "cross-env NODE_ENV=production RUN_CORDOVA=true webpack --progress && cd cordova && cordova run android",
    "build": "cross-env NODE_ENV=production RUN_CORDOVA=true webpack --progress && cd cordova && cordova build android --release",
    "keystore": "keytool -genkey -v -keystore cordova/hicordova.keystore -alias hicordova -keyalg RSA -validity 20000",
    "jarsigner": "jarsigner -verbose -keystore cordova/hicordova.keystore -signedjar cordova/platforms/android/app/build/outputs/apk/release/hicordova.apk cordova/platforms/android/app/build/outputs/apk/release/app-release-unsigned.apk hicordova"
  },
  "homepage": "https://github.com/kimseongrim/hicordova/blob/master/README.md",
